From f6c0a57c485080f0b2ac78d9e839191af54c57f4 Mon Sep 17 00:00:00 2001
From: Nathan Chancellor <nathan@kernel.org>
Date: Fri, 16 Jan 2026 16:44:05 -0700
Subject: [PATCH 15/15] HACK: LONGTERM: Disable Rust in 6.1

Some of the fixes for newer tools are not in this tree, breaking the
build in my Arch Linux build environment.

Signed-off-by: Nathan Chancellor <nathan@kernel.org>
---
 arch/x86/Kconfig | 1 -
 1 file changed, 1 deletion(-)

diff --git a/arch/x86/Kconfig b/arch/x86/Kconfig
index c7dfe2a5f162..af60084bbb6d 100644
--- a/arch/x86/Kconfig
+++ b/arch/x86/Kconfig
@@ -265,7 +265,6 @@ config X86
 	select HAVE_STATIC_CALL_INLINE		if HAVE_OBJTOOL
 	select HAVE_PREEMPT_DYNAMIC_CALL
 	select HAVE_RSEQ
-	select HAVE_RUST			if X86_64
 	select HAVE_SYSCALL_TRACEPOINTS
 	select HAVE_UACCESS_VALIDATION		if HAVE_OBJTOOL
 	select HAVE_UNSTABLE_SCHED_CLOCK
